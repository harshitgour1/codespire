#include <iostream>
#include <cmath>
using namespace std;

// Function to check if a number is prime
bool isPrime(int num) {
    if (num < 2) return false;
    for (int i = 2; i * i <= num; ++i) {
        if (num % i == 0) return false;
    }
    return true;
}

int main() {
    int D, P;
    cin >> D >> P;

    int partLength = D / P;
    int count = 0;

    // Iterate through each hour in the first part
    for (int hour = 0; hour < partLength; ++hour) {
        bool allPrime = true;

        // Check the corresponding hour in all parts
        for (int i = 0; i < P; ++i) {
            int equivalentHour = (hour + i * partLength) % D;
            if (!isPrime(equivalentHour)) {
                allPrime = false;
                break;
            }
        }

        // If all corresponding hours are prime, increment the count
        if (allPrime) {
            count++;
        }
    }

    cout << count << endl;
    return 0;
}
